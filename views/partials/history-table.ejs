<div class="history">
    <h2>History</h2>
    <hr>
    <ul>
        <% for (var entry in history) { %>
            <% if (history[entry].status == 'home') { %>
                <li>Returned on <%= history[entry].timestamp %></li>
            <% } else { %>
                <li>Departed on <%= history[entry].timestamp %></li>
            <% } %>
        <% } %>
    </ul>
</div>
